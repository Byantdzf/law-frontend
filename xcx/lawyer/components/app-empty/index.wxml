<!-- 无数据提示 -->
<view class="app-empty">
  <view class="app-empty__hd">
    <image src="{{ imgSrc }}" mode="widthFix" wx:if="{{ !iconClass }}"></image>
    <view class="iconfont {{ iconClass }}" wx:else></view>
  </view>
  <view class="app-empty__bd">
    <text>{{ tips }}</text>
  </view>
  <view class="app-empty__ft">
    <slot></slot>
  </view>
</view>